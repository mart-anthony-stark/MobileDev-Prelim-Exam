<?xml version='1.0' encoding='utf-8'?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group8.listapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:resizeableActivity="false">

		<activity
            android:name=".MenuActivity"
			android:label="@string/app_name">
			<intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
		
        <activity
            android:name=".MainActivity"
            android:label="List of names">
        </activity>

		<activity
            android:name=".MartActivity"
            android:label="Mart's Profile">
        </activity>
		
		<activity
            android:name=".HomerActivity"
            android:label="Homer's Profile">
        </activity>
		
		<activity
            android:name=".RebyActivity"
            android:label="Reby's Profile">
        </activity>
		
        <meta-data
          android:name="android.max_aspect"
          android:value="4.0"/>

    </application>

</manifest>
